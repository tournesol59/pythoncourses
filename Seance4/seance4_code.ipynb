    "# Classe de base (parent)\n",
    "class Animal:\n",
    "    def __init__(self, nom):\n",
    "        self.nom = nom\n",
    "\n",
    "    def parler(self):\n",
    "        print(f\"{self.nom} √©met un son.\")\n",
    "\n",
    "# Sous-classe (enfant) qui h√©rite de la classe de base\n",
    "class Chien(Animal):\n",
    "    def __init__(self, nom, race):\n",
    "        # Appeler le constructeur de la classe parente avec super()\n",
    "        super().__init__(nom)\n",
    "        self.race = race\n",
    "\n",
    "    # Red√©finir la m√©thode parler de la classe parente\n",
