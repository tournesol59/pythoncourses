    "import random\n",
    "\n",
    "class Carte:\n",
    "    def __init__(self, valeur, enseigne):\n",
    "        self.valeur = valeur\n",
    "        self.enseigne = enseigne\n",
    "\n",
    "    def __str__(self):\n",
    "        return f\"{self.valeur} de {self.enseigne}\"\n",
    "\n",
    "class Deck:\n",
    "    def __init__(self):\n",
    "        valeurs = ['2', '3', '4', '5', '6', '7', '8', '9', '10', 'Valet', 'Dame', 'Roi', 'As']\n",
    "        enseignes = ['C≈ìur', 'Pique', 'Carreau', 'Tr√®fle']\n",
    "        self.cartes = [Carte(valeur, enseigne) for valeur in valeurs for enseigne in enseignes]\n",
    "\n",
    "    def melanger(self):\n",
    "        random.shuffle(self.cartes)\n",
    "\n",
    "    def distribuer(self):\n",
    "        if len(self.cartes) > 0:\n",
    "            return self.cartes.pop()\n",
    "        else:\n",
    "            print(\"Le deck est vide. M√©langez le deck.\")\n",
    "            return None\n",
